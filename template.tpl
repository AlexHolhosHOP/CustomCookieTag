const storageType = data.storageType;
const customCookieName = data.cookieName;
const customCookieValue = data.cookieValue;
const queryPermission = require('queryPermission');
const setCookie = require('setCookie');
const getCookieValues = require('getCookieValues');

var options = {
	'domain': 'auto',
    'path': 'auto',
    'max-age': 60*60*24*365,
    'secure': true
};

if(storageType == 'Add Custom Cookie'){
  if(queryPermission('get_cookies', customCookieName) && getCookieValues(customCookieName)[0] !== customCookieValue){
    if (queryPermission('set_cookies', customCookieName, options)) {
      setCookie(customCookieName, customCookieValue, options);
    }
  }
}
else if(storageType == 'Remove Custom Cookie'){
  if(queryPermission('get_cookies', customCookieName) && getCookieValues(customCookieName)[0] !== undefined){
    options = {
      'domain': 'auto',
      'path': 'auto',
      'max-age': 0,
      'secure': true
    };
    if (queryPermission('set_cookies', customCookieName, options)) {
      setCookie(customCookieName, '', options);
    }
  }
}

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


